{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../dist/semantic-ui",
      "uncompressed": "../dist/semantic-ui/components/",
      "compressed": "../dist/semantic-ui/components/",
      "themes": "../dist/semantic-ui/themes/"
    },
    "clean": "../dist/semantic-ui/"
  },
  "components": [
   "reset",
   "site",
   "container",
   "grid",
   "icon",
   "search",
   "input",
   "button",
   "sidebar",
   "menu",
   "api"
  ],
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "version": "2.4.0"
}
